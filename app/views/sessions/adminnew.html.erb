<h1>Log in</h1>
<%=form_tag do%>
	<table class= "myformat">
		<tr>
			<td><%= label_tag :emp_number %></td>
			<td><%= text_field_tag :emp_number %>
		</tr>
		<tr>
			<td><%= label_tag :password %></td>
			<td><%= password_field_tag :password%>
		</tr>
		<tr>
			<td><%= submit_tag "Log in" %></td>
		</tr>
	</table>
<%end%>